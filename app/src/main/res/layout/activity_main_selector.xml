                        android:singleLine="false"
                        android:layout_alignBottom="@+id/testView2"
                        android:layout_toLeftOf="@+id/testView2"
                        android:layout_toStartOf="@+id/testView2" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Temp2:"
